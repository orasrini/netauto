pipeline {
  agent any
  stages {
    stage('Build') {
      steps {
        sh 'echo "this is the build stage"'
      }
    }

    stage('Test') {
      steps {
        sh 'echo "this is test stage"'
      }
    }

    stage('deploy') {
      steps {
        sh 'echo " this is deploy stage"'
      }
    }

  }
}